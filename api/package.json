{
  "name": "@mattrax/api",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "main": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./server": {
      "import": "./src/server.ts",
      "types": "./src/server.ts"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./src/index.ts"
      ],
      "server": [
        "./src/server.ts"
      ]
    }
  },
  "dependencies": {
    "@hono/zod-validator": "^0.1.11",
    "@mattrax/policy": "workspace:^",
    "@planetscale/database": "^1.14.0",
    "@t3-oss/env-core": "^0.7.3",
    "base58": "^2.0.1",
    "drizzle-orm": "^0.29.3",
    "hono": "^3.12.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@microsoft/microsoft-graph-types": "^2.40.0"
  }
}
